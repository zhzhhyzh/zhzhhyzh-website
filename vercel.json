{
  "version": 2,
  "cleanUrls": true,
  "routes": [
    { "src": "/api/(.*)", "dest": "/server.js" },
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/style.css", "dest": "/style.css" },
    { "src": "/script.js", "dest": "/script.js" },
    { "src": "/capture.js", "dest": "/capture.js" },
    { "src": "/pnc", "dest": "/pnc.html" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
